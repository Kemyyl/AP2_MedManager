@model IEnumerable<Medecin>
@{



    <html>

    <head>
        <title>Index</title>
    </head>

    <body>
        <h1>Liste des Medecin </h1>

        <table class="table table-dark table-striped-columns">

            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>Prenom</th>
                <th>Date de Naissance</th>
                <th>RÃ´le</th>
                <th>Details </th>
                <th>Modifier </th>
                <th>Supprimer </th>
            </tr>
            @foreach (var medecin in Model)
            {
                <tr>
                    <td>@medecin.MedecinId</td>
                    <td>@medecin.Nom_m</td>
                    <td>@medecin.Prenom_m</td>
                    <td>@medecin.Date_naissance_m</td>
                    <td>@medecin.Role</td>
                    <TD><a asp-action="Details" asp-route-id="@medecin.MedecinId"> <i class="bi bi-eye-fill"></i></a></TD>
                    <TD><a asp-action="Modifier" asp-route-id="@medecin.MedecinId"> <i class="bi bi-pencil-fill"></i></a></TD>
                    <TD><a asp-action="Supprimer" asp-route-id="@medecin.MedecinId"> <i class="bi bi-trash3-fill"></i></a></TD>
                </tr>
            }

        </table>
        <a asp-action="Ajouter" class="btn btn-primary">Ajouter un Medecin</a>
    </body>

    </html>
}
